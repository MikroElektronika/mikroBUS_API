const _MIKROBUS_ERR_UART          as byte = 5

' ------------------------------------------------------------ SYSTEM SPECIFIC '

#IFDEF ENABLE_UART THEN

sub function _uartInit_1 (dim cfg as ^const uint32_t) as T_mikrobus_ret   
dim arg1, arg2, arg3, arg4 as ^const uint32_t
    arg1 = (cfg + 0)
    arg2 = (cfg + 1)
    arg3 = (cfg + 2)
    arg4 = (cfg + 3)
    UART0_Init_Advanced(longword(arg1^), word(arg2^), word(arg3^), word(arg4^))
    Result = 0
    UART0_Write (0)
    if (UART0_Data_Ready() ) then
        UART0_Read ()
    end if
end sub

